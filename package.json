{
  "name": "graphql-microservice-demo",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start:backend": "concurrently  \"yarn start:backend01\" \"yarn start:backend03\" \"yarn start:gateway\"",
    "start:backend01": "nodemon --config ./service01/nodemonconfig.json",
    "start:backend03": "go run service-go/main.go",
    "start:gateway": "nodemon --config ./gateway-service/nodemonconfig.json",
    "start:frontend": "npx ng serve",
    "start:ppt": "npx nodeppt serve nodeppt.md",
    "start": "npx nodeppt serve nodeppt.md",
    "build": "npx nodeppt build ./nodeppt.md",
    "test": "npx ng test",
    "lint": "npx ng lint",
    "e2e": "npx ng e2e",
    "ss": "concurrently \"npm:start-docker-compose\" \"npm:start-backend\" ",
    "start-docker-compose": "docker-compose up",
    "codegen": "npx graphql-codegen",
    "schema:download": "npx apollo service:download --endpoint=http://localhost:4000/graphql",
    "schema:generate": "npx apollo codegen:generate --endpoint=http://localhost:4000/graphql --includes='src/app/api/**/*.graphql' --target=typescript",
    "gen:types": "yarn schema:download && schema:generate"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.7",
    "@angular/cdk": "~8.2.1",
    "@angular/common": "~8.2.7",
    "@angular/compiler": "~8.2.7",
    "@angular/core": "~8.2.7",
    "@angular/forms": "~8.2.7",
    "@angular/material": "^8.2.1",
    "@angular/platform-browser": "~8.2.7",
    "@angular/platform-browser-dynamic": "~8.2.7",
    "@angular/router": "~8.2.7",
    "@graphql-codegen/introspection": "^1.8.1",
    "@graphql-codegen/typescript": "^1.8.1",
    "@graphql-codegen/typescript-apollo-angular": "^1.8.1",
    "@graphql-codegen/typescript-compatibility": "^1.8.1",
    "@graphql-codegen/typescript-operations": "^1.8.1",
    "@types/koa-graphql": "^0.8.3",
    "@types/koa-router": "^7.0.42",
    "@types/lodash-es": "^4.17.3",
    "apollo-link-context": "^1.0.17",
    "apollo-link-http": "^1.5.14",
    "bootstrap": "^4.3.1",
    "concurrently": "^4.1.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "lodash-es": "^4.17.15",

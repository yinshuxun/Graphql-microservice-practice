    "@angular/compiler-cli": "~8.2.7",
    "@angular/language-service": "~8.2.7",
    "@graphql-codegen/cli": "^1.8.1",
    "@types/graphql": "^14.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/koa": "^2.0.48",
    "@types/mysql": "^2.15.7",
